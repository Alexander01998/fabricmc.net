        }
        function u(t2, e2, n2, r2) {
            var i2 = e2.split("\n"), a2 = Math.max(n2 - 3, 0), s2 = Math.min(i2.length, n2 + 3), o2 = r2, l2 = i2.slice(a2, s2).map(function(t3, e3) {
